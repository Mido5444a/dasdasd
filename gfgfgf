local AimingTab = Window:CreateTab("ÿßŸÑÿ™ŸàŸÑ") --you can rename this tab to whatever you want --you can also change the tabs code, for example "AimingTab" can be changed to "FunnyCoolTab" etc.
local testSection = AimingTab:CreateSector("ÿßŸÑÿ±ÿßÿØŸäŸà", "right")  --you can  change the section code, for example "testsection" can be changed to "FunnyCoolSection" etc.

function grabtools()
  local speaker = game.Players.LocalPlayer
  local Human = speaker.Character:FindFirstChildWhichIsA("Humanoid")
  for _, v in ipairs(workspace:GetChildren()) do
    if speaker.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
      Human:EquipTool(v)
    end
  end
  if grabtoolsFunc then grabtoolsFunc:Disconnect() end
  grabtoolsFunc = workspace.ChildAdded:Connect(function(v)
    if speaker.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
      speaker.Character:WaitForChild("Humanoid"):EquipTool(v)
    end
  end)
end


testSection:AddToggle("ÿ®ÿßŸÑÿßÿ±ÿ∂ ÿ±ÿßÿØŸäŸà ÿßŸä ÿ≥ÿ≠ÿ®",false ,function(Value)
  if Value == true then
    grabtools()
  elseif Value == false then
    if grabtoolsFunc then grabtoolsFunc:Disconnect() end
  end
end)


function drop()
  _G.Drop = true
    while _G.Drop == true do
        game.Players.LocalPlayer.Backpack:FindFirstChildOfClass("Tool").Parent = game.Players.LocalPlayer.Character
    wait()
    game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Parent = game.Workspace
    local args = {
        [1] = ";re"
    }
    
    game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(unpack(args))
    wait(1.1)
end
end

testSection:AddToggle("ÿßŸÑÿ±ÿßÿØŸäŸà ÿ™ÿØÿ®ŸäŸÑ",false ,function(Value)
  if Value == true then
    drop()
  elseif Value == false then
    _G.Drop = false
    end
end)

stopDupe = true
local speaker = game.Players.LocalPlayer
local radio = "\216\167\217\132\216\167\216\186\216\167\217\134\217\138 \217\133\216\179\216\172\217\132"
function Dupe(amount)
    local DropT = {}
    speaker.Character.Humanoid:UnequipTools()
    stopDupe = false
    local Pos = speaker.Character.HumanoidRootPart.CFrame
    wait(.1)
    speaker.Character:MoveTo(Vector3.new(0, 1e7, 0))
    wait(.15)
    speaker.Character.HumanoidRootPart.Anchored = true
    for i = 1, amount do
        if stopDupe then
            break
        end
        speaker.Character:MoveTo(Vector3.new(0, 1e7, 0))
        wait(.3)
        for i, v in pairs(speaker.Backpack:GetChildren()) do
            if v:IsA("Tool") and v.Name == radio then
                v.Parent = speaker.Character
                wait(0.1)
                v.Parent = workspace
                table.insert(DropT, v)
            end
        end
        wait(.3)
        print("")
        local args = {
            [1] = ";respawn "
        }

        repeat 
            game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(unpack(args)) wait()
        until speaker.CharacterAdded:wait()
        wait(.3)
        speaker.Character:MoveTo(Vector3.new(Pos))
        wait(.3)
        if stopDupe then
            break
        end
    end
    local Human = speaker.Character:FindFirstChildWhichIsA("Humanoid")
    for _, v in ipairs(workspace:GetChildren()) do
        if speaker.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
            Human:EquipTool(v)
        end
    end
    if grabtoolsFunc then grabtoolsFunc:Disconnect() end 
    grabtoolsFunc = workspace.ChildAdded:Connect(function(v)
        if speaker.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
            speaker.Character.Humanoid:EquipTools()
        end
    end)
    wait()
    if grabtoolsFunc then grabtoolsFunc:Disconnect() end
    speaker.Character.Humanoid:UnequipTools()
    for i, v in pairs(DropT) do
        firetouchinterest(v.Handle, speaker.Character.HumanoidRootPart, 0)
        repeat
            wait()
        until v.Parent == speaker.Character
    end
    speaker.Character.HumanoidRootPart.CFrame = Pos
end
function getRoot(char)
    local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
    return rootPart
end

function tools(plr)
    if plr:FindFirstChildOfClass("Backpack"):FindFirstChildOfClass('Tool') or plr.Character:FindFirstChildOfClass('Tool') then
        return true
    end
end

function grabtools()
    local Human = speaker.Character:FindFirstChildWhichIsA("Humanoid")
    for _, v in ipairs(workspace:GetChildren()) do
        if speaker.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
            Human:EquipTool(v)
        end
    end
    if grabtoolsFunc then grabtoolsFunc:Disconnect() end
    grabtoolsFunc = workspace.ChildAdded:Connect(function(v)
        if speaker.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
            speaker.Character.Humanoid:EquipTools()
        end
    end)
end
function ungrabtools()
     if grabtoolsFunc then grabtoolsFunc:Disconnect() end
end
spawn(function ()
    function dropped()
    if speaker.Character then
        for _,obj in pairs(speaker.Character:GetChildren()) do
            if obj:IsA("Tool") then
                obj.CanBeDropped = true
            end
        end
    end
    if speaker:FindFirstChildOfClass("Backpack") then
        for _,obj in pairs(speaker:FindFirstChildOfClass("Backpack"):GetChildren()) do
            if obj:IsA("Tool") then
                obj.CanBeDropped = true
            end
        end
    end
end
end)

