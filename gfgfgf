hh.pr1.Size = Vector3.new(5,3,1)


for i,v in pairs (Character.Hand:GetChildren()) do
    if v:IsA("Part") then
        v.CanCollide = false
        v.Transparency = 1
        v.CFrame = Character.HumanoidRootPart.CFrame
    end
end

