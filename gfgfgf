      location1 = Vector3.new(170829.656, 22178.2383, -97589.8516, -0.963841319, -0.0994151235, 0.247237772, 8.34072456e-09, 0.927802265, 0.373072386, -0.26647681, 0.359582573, -0.894254148), -- your desired position
  }
  
